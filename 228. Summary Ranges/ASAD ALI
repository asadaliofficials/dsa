/**
 * @param {number[]} nums
 * @return {string[]}
 */
var summaryRanges = function (nums) {
	const result = [];
	let start = 0;

	for (let i = 1; i <= nums.length; i++) {

		if (i === nums.length || nums[i] !== nums[i - 1] + 1) {
            
			if (i - start === 1) {
				result.push(nums[start].toString());
			} else {
				result.push(`${nums[start]}->${nums[i - 1]}`);
			}

			start = i;
		}
	}

	return result;
};
