/**
 * @param {string} s
 * @return {character}
 */
var repeatedCharacter = function(s) {
    let seen = new Set();
    
    for (let char of s) {
        if (seen.has(char)) {
            return char;
        }
        seen.add(char);
    }
    
    // According to constraints, we always have at least one repeated letter
    // so this return won't be reached, but it's here for completeness.
    return '';
};